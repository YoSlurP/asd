<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.hirnok.HelloController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
   <children>
      <Pane fx:id="pnJatek" onMousePressed="#onMousePressed" prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #ccddff; -fx-border-color: black;">
         <children>
            <ImageView fx:id="ivPlayer" fitHeight="72.0" fitWidth="72.0" layoutX="100.0" layoutY="100.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@player.png" />
               </image>
            </ImageView>
            <ImageView fx:id="ivBad" fitHeight="72.0" fitWidth="72.0" layoutX="700.0" layoutY="100.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@bad.png" />
               </image>
            </ImageView>
            <ImageView fx:id="ivHome" fitHeight="96.0" fitWidth="96.0" layoutX="100.0" layoutY="500.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@home.png" />
               </image>
            </ImageView>
            <ImageView fx:id="ivPapir" fitHeight="64.0" fitWidth="64.0" layoutX="116.0" layoutY="253.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@paper.png" />
               </image>
            </ImageView>
         </children>
      </Pane>
      <HBox alignment="CENTER_LEFT" spacing="5.0">
         <children>
            <ImageView fitHeight="64.0" fitWidth="64.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@paper.png" />
               </image></ImageView>
            <Label fx:id="lbdarab" maxHeight="1.7976931348623157E308" text=" 0 db">
               <font>
                  <Font name="System Bold" size="36.0" />
               </font></Label>
            <ProgressBar fx:id="pbHealth" maxWidth="1.7976931348623157E308" progress="1.0" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
   </children>
</VBox>
